<?xml version="1.0"?>
<config>
    <modules>
        <Cammino_Productstocksort>
            <version>0.1.0</version>
        </Cammino_Productstocksort>
    </modules>
    <global>
        <models>
            <productsort>
                <class>Cammino_Productstocksort_Model</class>
            </productsort>
        </models>
    </global>
    <frontend>
        <events>
            <catalog_product_collection_load_before>
                <observers>
                    <productsort_observer>
                        <type>singleton</type>
                        <class>productsort/observer</class>
                        <method>catalogProductCollectionLoadBefore</method>
                    </productsort_observer>
                </observers>
            </catalog_product_collection_load_before>
        </events>
    </frontend>
</config>